<!DOCTYPE html>
<html class="um landscape min-width-240px min-width-320px min-width-480px min-width-768px min-width-1024px">
    <head>
        <title></title>
        <meta charset="utf-8">
        <meta name="viewport" content="target-densitydpi=device-dpi, width=device-width, initial-scale=1, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
        <link rel="stylesheet" href="../build/css/fonts/font-awesome.min.css">
        <link rel="stylesheet" href="../build/css/ui-box.css">
        <link rel="stylesheet" href="../build/css/ui-base.css">
        <link rel="stylesheet" href="../build/css/ui-color.css">
        <link rel="stylesheet" href="../build/css/appcan.icon.css">
        <link rel="stylesheet" href="../build/css/appcan.control.css">
        <link rel="stylesheet" href="css/manager.css">
        <script src="../common/common.js"></script>
    </head>
    <body class="um-vp bc-bg ub-ver" ontouchstart>
        <div class="ub ub-pj item-height1 sc-bg mf-size1" id="">
            <div class="umar-a">
                <div id="empName"></div>
                <div id="orgName" class="umar-t"></div>
            </div>
            <div class="ub uinn3">
                <div class="ub ub-ac ub-pc mf-size2"  id="logout">
                    注销
                    <span id="" class="fa fa-angle-right"> </span>
                </div>
            </div>
        </div>

        <div class="gap" id=""></div>

        <div class="item-height-intention sc-bg">
            <div id="listview"  class="sc-bg m-list"></div>
            <div class="ub ub-ac ub-pc padding-a1">
                <div id="0" class="ub ub-ver ub-ac ub-f1" onclick="viewDD(id)">
                    <div class="ui-s-state1 state1-tbd"></div>
                    <div class="mf-size2">
                        审核订单
                    </div>
                </div>
                <div id="1" class="ub ub-ver ub-ac ub-f1" onclick="viewDD(id)">
                    <div class="ui-s-state1 state1-pay"></div>
                    <div class="mf-size2">
                        确认订单
                    </div>
                </div>
                <div id="2" class="ub ub-ver ub-ac ub-f1" onclick="viewDD(id)">
                    <div class="ui-s-state1 state1-exchange"></div>
                    <div class="mf-size2">
                        销售结算
                    </div>
                </div>
                <div id="3" class="ub ub-ver ub-ac ub-f1" onclick="viewDD(id)">
                    <div class="ui-s-state1 state1-cancel"></div>
                    <div class="mf-size2">
                        配送自提审核
                    </div>
                </div>

            </div>

        </div>

        <div class="gap" id=""></div>
        <div class="ub ub-ver sc-bg item-height-cus" id="">
            <div class="ub ub-ac umar-l padding-a mf-size2">
                客户管理
            </div>
            <div class="ub ubt  bc-border ub-ac padding-a1">
                <div id="0" class="ub ub-ver ub-ac ub-pc ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-delivery"></div>
                    <div class="mf-size2">
                        所属分公司管理
                    </div>
                </div>
                <div id="1" class="ub ub-ver ub-ac ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-order"></div>
                    <div class="mf-size2">
                        客户资金管理
                    </div>
                </div>
                <div id="2" class="ub ub-ver ub-ac ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-trace"></div>
                    <div class="mf-size2">
                        个人注册审核
                    </div>
                </div>

            </div>
            <div class="ub bc-border ub-ac padding-a1">
                <div id="3" class="ub ub-ver ub-ac ub-pc ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-delivery"></div>
                    <div class="mf-size2">
                        注册客户管理
                    </div>
                </div>
                <div id="4" class="ub ub-ver ub-ac ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-order"></div>
                    <div class="mf-size2">
                        车辆审核
                    </div>
                </div>
                <div id="5" class="ub ub-ver ub-ac ub-f1" onclick="cusMgmt(id)">
                    <div class="ui-s-state state-order"></div>
                    <div class="mf-size2">
                        客户等级管理
                    </div>
                </div>

            </div>

        </div>
        <div class="gap" id=""></div>

        <div class="ub ub-ver sc-bg item-height2" id="">
            <div class="ub ub-ac umar-l padding-a mf-size2">
                提油管理
            </div>

            <div class="ub ubt  bc-border ub-ac padding-a1">
                <div id="0" class="ub ub-ver ub-ac ub-pc ub-f1" onclick="deliveryMgmt(id)">
                    <div class="ui-s-state state-delivery"></div>
                    <div class="mf-size2">
                        配送申请
                    </div>
                </div>
                <div id="1" class="ub ub-ver ub-ac ub-f1" onclick="deliveryMgmt(id)">
                    <div class="ui-s-state state-order"></div>
                    <div class="mf-size2">
                        预约申请
                    </div>
                </div>
                <div id="2" class="ub ub-ver ub-ac ub-f1" onclick="deliveryMgmt(id)">
                    <div class="ui-s-state state-trace"></div>
                    <div class="mf-size2">
                        付油跟踪
                    </div>
                </div>

            </div>
        </div>

        <div class="gap" id=""></div>

        <div class="ub ub-ver item-height3 sc-bg">
            <div class=" ub-f1 ub ub-ver mf-size2">
                <!--分类开始-->
                <div class="ub ub-ver ub-f1">
                    <div class="ub-f1 ub">
                        <div id="0" class="uw-app1 c-org-me01 ub ub-ac  ubr ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-file">
                                付油通知单
                            </div>
                        </div>
                        <div id="1" class="uw-app1 c-org-me01 ub ub-ac  ubr ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-user">
                                我的客户
                            </div>
                        </div>
                        <div id="2" class="uw-app1 c-gra-me01 ub ub-ac  ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-price">
                                库销价格
                            </div>
                        </div>
                    </div>
                    <div class="ub-f1 ub">
                        <div id="3" class="uw-app1 c-gra-me01 ub ub-ac  ubr ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-chart">
                                客户优惠
                            </div>
                        </div>
                        <div id="4" class="uw-app1 c-gra-me01 ub ub-ac ubr ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-report">
                                投诉管理
                            </div>
                        </div>
                        <div id="5" class="uw-app1 c-gra-me01 ub ub-ac ubb bc-border" onclick="openpage(id)">
                            <div class="ulev-me1 ub-f1 ui-manager ui-discount">
                                促销活动
                            </div>
                        </div>
                    </div>
                </div>
                <!--分类结束-->
            </div>
        </div>

        <script src="../build/js/appcan.js"></script>
        <script src="../build/js/appcan.control.js"></script>
        <script src="../build/js/appcan.listview.js"></script>
    </body>
    <script>
        var empName = "";
        var orgName = "";
        appcan.ready(function() {
            var orgInfo = appcan.locStorage.getVal("orgInfo");
            orgInfo = eval("(" + orgInfo + ")");
            empName = orgInfo.empName;
            orgName = orgInfo.orgName;
            $("#empName")[0].innerHTML = empName;
            $("#orgName")[0].innerHTML = orgName;
        });

        var lv_yx = appcan.listview({
            selector : "#listview",
            type : "thinLine",
            hasIcon : false,
            hasAngle : true,
            hasSubTitle : true,
            multiLine : 1,
        });
        lv_yx.set([{
            title : " <div class='ub ub-ac padding-a mf-size2'>订单管理</div>",
            subTitle : "全部订单",
            id : "1"
        }])
        lv_yx.on("click", function(ele, obj, curEle) {
            appcan.window.open("intention", "../intention/intention.html", 10);
        })
        // 订单管理
        function viewDD(id) {
            switch (id) {
            case "0":
                // 审核订单
                appcan.window.open("toAudit", "../intention/toAudit.html", 10);
                break;
            // 确认订单
            case "1":
                appcan.window.open("toConfirm", "../intention/toConfirm.html", 10);
                break;
            case "2":
                // 结算转付油
                appcan.window.open("toTransferOil", "../intention/toTransferOil.html", 10);
                break;
            case "3":
                // 配送自提
                appcan.window.open("pszt", "../intention/pszt.html", 10);
                break;
            }
        }

        // 提油管理
        function deliveryMgmt(id) {
            if (id) {
                appcan.window.alert({
                    title : "提示",
                    content : "该功能模块暂时未开通,敬请期待",
                    buttons : ['我支持'],
                    callback : function(err, data, dataType, optId) {

                        console.log(err, data, dataType, optId);
                    }
                });
            }
        }

        // 客户管理
        function cusMgmt(id) {
            if (id) {
                appcan.window.alert({
                    title : "提示",
                    content : "该功能模块暂时未开通,敬请期待",
                    buttons : ['我支持'],
                    callback : function(err, data, dataType, optId) {

                        console.log(err, data, dataType, optId);
                    }
                });
                return;
            }
            switch (id) {
            case "0":
                // 所属分公司
                appcan.window.open("cus", "../cus/belongtoFgs.html", 10);
                break;
            case "1":
                // 客户资金
                appcan.window.open("cus", "../cus/fundingreview.html", 10);
                break;
            case "2":
                // 个人注册审核
                appcan.window.open("cus", "../cus/personRegistered.html", 10);
                break;
            case "3":
                // 注册客户管理
                appcan.window.open("cus", "../cus/registeringCus.html", 10);
                break;
            case "4":
                // 车辆审核
                appcan.window.open("cus", "../cus/verhicle.html", 10);
                break;
             case "5":
             appcan.window.alert({
                    title : "提示",
                    content : "该功能模块暂时未开通,敬请期待",
                    buttons : ['我支持'],
                    callback : function(err, data, dataType, optId) {

                        console.log(err, data, dataType, optId);
                    }
                });
                break;

            }
        }

        function openpage(id) {
            if (id) {
                appcan.window.alert({
                    title : "提示",
                    content : "该功能模块暂时未开通,敬请期待",
                    buttons : ['我支持'],
                    callback : function(err, data, dataType, optId) {

                        console.log(err, data, dataType, optId);
                    }
                });
            }
        }


        appcan.button("#logout", "btn-act", function() {
            var url = URL + "/app/admin/loginout";
            var paramJsonStr = "";
            var func = logoutCallback;
            var dataType = "text";
            ajaxPostQuery(url, paramJsonStr, func, dataType);
        })
        function logoutCallback(data) {
            if (data == "ok") {
                appcan.locStorage.remove("sid");
                appcan.locStorage.setVal("status", "out");
                uexWindow.evaluateScript("manager", 0, "openindex()");
            } else {

            }
        }

    </script>
</html>
