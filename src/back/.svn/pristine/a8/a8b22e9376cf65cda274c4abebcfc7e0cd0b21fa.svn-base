input[type="range"] {
    -webkit-appearance: none;
    outline: none;
    background-color: #ddd;
    border-radius: 0.6em;
    width: 100%;
    height: 0.6em;
}
input[type="range"]::-webkit-slider-thumb {
    -webkit-appearance: none;
    cursor: default;
    height: 2.5em;
    width: 1.5em;
    border-radius: 1em;
    background: linear-gradient(#ffb1b1, #ff6d6d);
    transform: translateY(-0.1em);
    -webkit-box-shadow: 0 -1px 1px #d84646 inset;
}

