<script>
    appcan.ready(function () {
        //去除滚动条
        uexWindow.setWindowScrollbarVisible('false');
        var titHeight = $('#header').offset().height;
        appcan.frame.open("content", "delivery_content.html", 0, titHeight);
    });
    appcan.button(".nav-btn", "btn-act", function () {
        appcan.window.close(-1);
        removeVal("siteParam");
    })

    appcan.button("#tjqr", "btn-act", function () {
        uexWindow.evaluatePopoverScript("", "content", "subPSInfo()");
    })
    //打开配送时间选择
    function opentimeChoose() {
        openPopPage("dateChoose");
    }

    function closePage() {
        appcan.locStorage.setVal("Refresh", "true")
        appcan.window.publish("peisongRefresh", "true");

        appcan.window.close(-1);
        uexWindow.evaluateScript("mentionOil", 0, "getNewAllNotice()");

    }
</script>

