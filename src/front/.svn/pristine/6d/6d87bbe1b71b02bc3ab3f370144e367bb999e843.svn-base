    <script>
        appcan.ready(function() {
            var titHeight = $('#header').offset().height;
            appcan.frame.open("content", "managerEvaluate_content.html", 0, titHeight);
        });

        appcan.button("#nav-left", "ani-act", function() {
            appcan.window.close(-1);
        })
        
        appcan.button("#manEvaluate", "ani-act", function(){
            uexWindow.evaluatePopoverScript("", "content", "manEvaluate()");
        })
        
        function finishEvaluate(){
            appcan.window.close(-1);
            appcan.window.publish("MEF","manager");
        }

</script>
