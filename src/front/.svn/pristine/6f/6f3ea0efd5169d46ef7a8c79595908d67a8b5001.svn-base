<!DOCTYPE html>
<html class="um landscape min-width-240px min-width-320px min-width-480px min-width-768px min-width-1024px">
    <head>
        <title></title>
        <meta charset="utf-8">
        <meta name="viewport" content="target-densitydpi=device-dpi, width=device-width, initial-scale=1, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
        <link rel="stylesheet" href="../build/css/fonts/font-awesome.min.css">
        <link rel="stylesheet" href="../build/css/ui-box.css">
        <link rel="stylesheet" href="../build/css/ui-base.css">
        <link rel="stylesheet" href="../build/css/ui-color.css">
        <link rel="stylesheet" href="../build/css/appcan.icon.css">
        <link rel="stylesheet" href="../build/css/appcan.control.css">
        <link rel="stylesheet" href="css/search.css">
    </head>
    <body class="white-bg" ontouchstart >
        <div class="white-bg" tabindex="0" data-control="PAGE" id="Page" data-bind="">
            <div class="uh bc-head  ubb bc-border" data-control="HEADER" id="Header">
                <div class="ub">
                    <div class="nav-btn" id="nav-left">
                        <div class="fa fa-angle-left fa-2x"></div>
                    </div>
                    <h1 class="ut ub-f1 ulev-3 ut-s tx-c" tabindex="0">快速查询</h1>
                    <div class="nav-btn" id="nav-right">
                        <div class="ub-img1 icon-adduser ulev2"></div>
                    </div>
                </div>
            </div>
            <div class="ub-f1 tx-l" data-control="CONTENT" id="ScrollContent" style="padding:0">
                <div class="scrollbox">
                    <div class="box_bounce ub ub-ver ub-pc">

                        <div class="umar-a">
                            <div class="sc-bg-active uinput ub ub-f1  bc-border uba" style="border-radius: 0.4em;">
                                <div class="uinn fa fa-search sc-text"></div>
                                <input placeholder="请输入关键字" type="text" class="ub-f1">
                            </div>
                            <div class="uinn ubb bc-border">
                                <div class="ulev-app2">
                                    单据搜索
                                </div>
                                <div class="ub umar-a">
                                    <div id="0" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        我的订单
                                    </div>
                                    <div id="1" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        付油通知单
                                    </div>
                                    <div id="2" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        提货单
                                    </div>

                                </div>
                                <div class="ub umar-a">
                                    <div id="3" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        出库单
                                    </div>
                                    <div id="4" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        销售结算单
                                    </div>

                                    <div id="5" onclick="openPage(id)"  class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        增值发票
                                    </div>
                                </div>
                            </div>
                            <div class="uinn ubb bc-border">
                                <div class="ulev-app2">
                                    其他查询
                                </div>
                                <div class="ub umar-a">
                                    <div id="6" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        交易记录
                                    </div>
                                    <div id="7" onclick="openPage(id)" class="primary uba bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">
                                        客存
                                    </div>

                                    <div class="bc-border ub-f1 uinn-d umar-2 ub ub-ac ub-pc min-w5 ulev-1">

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
        <script src="../build/js/appcan.js"></script>
        <script src="../build/js/appcan.control.js"></script>
        <script src="../build/js/appcan.listview.js"></script>

        </script>
        <script src="js/search.js"></script>

        <script type="text/javascript" charset="utf-8">
            appcan.button("#nav-left", "btn-act", function() {
                appcan.window.close(-1);
            })
            function openPage(id) {

                var username = appcan.locStorage.getVal("username");

                if (!username) {

                    appcan.window.alert({
                        title : "提示",
                        content : "先登录再查看",
                        buttons : ['立即登录', '取消'],
                        callback : function(err, data, dataType, optId) {
                            if (data == '0') {
                                appcan.locStorage.setVal("tag", "listpage");
                                appcan.window.open("login", "../userpage/login.html", 10);
                            }
                            if (data == '1') {

                            }
                            console.log(err, data, dataType, optId);
                        }
                    });
                    return;

                }

                switch(id) {
                case "0":
                    appcan.window.open("search_dingdan", "../buyOil/intentionList/intentionList.html", 10, 0);
                    break;
                case "1":
                    appcan.window.open("ordermgmt", "ordermgmt.html", 10, 0);
                    break;
                case "2":
                    appcan.window.open("pickup", "search_pickup.html", 10, 0);
                    break;
                case "3":
                    appcan.window.open("chuku", "search_chuku.html", 10, 0);
                    break;
                case "4":
                    appcan.window.open("jiesuan", "search_jiesuan.html", 10, 0);
                    break;
                case "6":
                    appcan.window.open("zijing", "../myAccount/funds/balance.html", 10, 0);
                    break;
                case "7":
                    appcan.window.open("kecun", "cus_deposit.html", 10, 0);
                    break;
                default:
                    appcan.window.alert({
                        title : "提示",
                        content : "该功能模块暂时未开通,敬请期待",
                        buttons : ['我支持'],
                        callback : function(err, data, dataType, optId) {

                            console.log(err, data, dataType, optId);
                        }
                    });
                }
            }

        </script>

    </body>
</html>