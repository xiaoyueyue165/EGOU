/*打款布局*/
.wrapper {
    background-color: #eee;
    width: 90%;
    position: absolute;
    left: 5%;
    top: 6em;
    border-radius: 3%;
    display: none;
    opacity: 0.95;
    padding-bottom: 1em;
    border: 1px solid #ccc;
}
.margin-t-2x {
    margin: 1em;
    position: relative;
}
.m-input {
    width: 6.625em;
    font-zize: 2em !important;
    box-sizing: border-box;
    height: 1.5em;;
    border: 0;
    border: 1px solid #999;
}

.select {
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
}

.m-input span {
    color: #000;
}
.mySelect option {
    font-zize: 1em !important;
}
.action-group {

    text-align: center;
}
.selected {
    border: 1px solid #000 !important;
    padding: 0.2em;
    border-radius: 10%;
}
#chk {

    width: 4em;
    height: 4em;
    border: 1px solid #999;
}
.m-btn {
    width: 4em;
    height: 2em;
    font-size: 1em;
    text-align: center;
    border-radius: 10%;
    border: 0;
    border: 1px solid #999;
}
#peisong, #ziti {
    border: 1px solid transparent;
    padding: 0.2em;
}
.chuku {
    position: absolute;
    left: 5.2em;
    top: 0;
    line-height: 1.8em;
}
.um3 {
    margin-left: 3em;
}
#assets {
    margin-top: 0.3em;;
}
.icon :before {
    width: 1em;
    height: 1em;
    display: -webkit-box !important;
    display: box !important;
    position: relative;
    -webkit-box-align: center;
    box-align: center;
    -webkit-box-pack: center;
    box-pack: center;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: 1.5em;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\f107";
}

