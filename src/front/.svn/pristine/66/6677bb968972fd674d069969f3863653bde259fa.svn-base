        var LayoutIndex = {
            options : function() {
                var y = this,
                    options = {};
                y.options.homeBtns = document.getElementById('footer');
                y.options.body = document.body;

                return y;
            },
            init : function() {
                var y = this;
                document.body.appendChild(this.getLayoutScript());

                this.options();
            },
            bind : function() {

            },
            appendButton : function() {
                // var y = this;
                // var button = document.createElement('button');
