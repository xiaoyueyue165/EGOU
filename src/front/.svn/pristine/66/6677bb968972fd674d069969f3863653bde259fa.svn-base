                // y.options.homeBtns.appendChild(button);
                // button.onclick = getAppVersion;
                // button.onclick();
                // button.onclick();
            },
            getLayoutScript : function() {
                var u = navigator.userAgent,
                    script = document.createElement('script'),
                    device = "";
                //当前设备信息
                if (u.indexOf('Android') > -1 || u.indexOf('Linux') > -1) {//安卓手机
                    device = "Android";
                    script.src = "build/compatible/Android.js";

                } else if (u.indexOf('iPhone') > -1) {//苹果手机
                    device = "iPhone";
                    script.src = "build/compatible/Ios.js"

                } else if (u.indexOf('Windows Phone') > -1) {//winphone手机
                    device = "WindowsPhone";
                } else {
                    script.src = "build/compatible/Android.js";
                }
                return script;
            }
        }

        LayoutIndex.init();

    </script>

